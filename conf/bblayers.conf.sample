# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  /workdir/layers/poky/meta \
  /workdir/layers/poky/meta-poky \
  /workdir/layers/poky/meta-yocto-bsp \
  /workdir/layers/meta-rauc-community/meta-rauc-qemux86 \
  /workdir/layers/meta-rauc \
  /workdir/layers/meta-rauc-qemu-demo  \
  "
